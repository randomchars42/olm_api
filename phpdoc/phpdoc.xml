<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>Olm Api</title>
	<parser>
		<target>./output</target>
		<encoding>utf8</encoding>
		<markers>
			<item>TODO</item>
			<item>FIXME</item>
		</markers>
		<extensions>
			<extension>php</extension>
		</extensions>
